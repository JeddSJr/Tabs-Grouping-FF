{
    "name": "Tabs Grouping",
    "description": "Group your tabs by title, URL or domain name.",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "author": "13BPs",
    "browser_specific_settings": {
        "gecko": {
            "id": "Tabs@Grouping.com"
        }
    },
    "action": {
        "default_popup": "src/popup/interface.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}